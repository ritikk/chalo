<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget id="com.ritikk.chalo" version="0.0.1" xmlns="http://www.w3.org/ns/widgets"
        xmlns:cdv="http://cordova.apache.org/ns/1.0"
        xmlns:gap = "http://phonegap.com/ns/1.0">
    <name>CHALO</name>
    <description>
        CHALO let's you co-ordinate meetups
    </description>
    <author email="crucio.ritik@gmail.com" href="http://ritikkhatwani.blogspot.com">
        Ritik Khatwani
    </author>
    <content src="index.html"/>
    <access origin="*"/>
    <preference name="webviewbounce" value="false"/>
    <preference name="UIWebViewBounce" value="false"/>
    <preference name="DisallowOverscroll" value="true"/>
    <preference name="BackupWebStorage" value="none"/>
    <preference name="orientation" value="portrait" />
    <feature name="StatusBar">
        <param name="ios-package" value="CDVStatusBar" onload="true"/>
    </feature>
    <gap:plugin name="org.apache.cordova.contacts" version="0.2.13" />
    <gap:platform name="ios" />
</widget>